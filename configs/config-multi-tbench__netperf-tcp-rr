### tbench4 is the network portion of the dbench4 benchmark.

export MMTESTS="multi"
export MULTI_TESTS="netperf-tcp-rr tbench4"

# List of monitors
export RUN_MONITOR=yes
export MONITORS_ALWAYS=
export MONITORS_GZIP="proc-vmstat mpstat"
export MONITORS_WITH_LATENCY="vmstat"
export MONITOR_UPDATE_FREQUENCY=10

# TBench
export TBENCH_DURATION=600
export TBENCH_MIN_CLIENTS=$NUMCPUS
export TBENCH_MAX_CLIENTS=$NUMCPUS

# NetPerf TCP_RR
export NETPERF_BUFFER_SIZES=1
export NETPERF_ITERATIONS=5

export MULTI_SHUTDOWN_TBENCH=yes
