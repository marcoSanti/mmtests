export MMTESTS="lmbench-lat_proc"

# List of monitors
export RUN_MONITOR=yes
export MONITORS_ALWAYS=
export MONITORS_GZIP="proc-vmstat mpstat turbostat"
export MONITORS_WITH_LATENCY="vmstat"
export MONITOR_PERF_EVENTS=cpu-migrations,context-switches
export MONITOR_UPDATE_FREQUENCY=10

# LMBench
export LMBENCH_LATPROC_ITERATIONS=10
export LMBENCH_LATPROC_MIN_PROCS=1
export LMBENCH_LATPROC_MAX_PROCS=$NUMCPUS
