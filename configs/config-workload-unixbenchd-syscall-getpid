export MMTESTS="unixbenchd"

# List of monitors
export RUN_MONITOR=yes
export MONITORS_ALWAYS=
export MONITORS_GZIP="proc-vmstat top mpstat turbostat"
export MONITORS_WITH_LATENCY="vmstat"
export MONITOR_UPDATE_FREQUENCY=10

# unixbenchd
export UNIXBENCHD_ITERATIONS=30
export UNIXBENCHD_DURATION=5
export UNIXBENCHD_SUBCOMMAND=syscall
export UNIXBENCHD_SUBPARAM=getpid
